import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner A=new Scanner(System.in);
        String a=A.nextLine();
        String b=A.nextLine();      
        String c=a.concat(b);
        String d=c.replace(" ","");
        System.out.println(d);       
    }
}
