import java.io.*;
import java.util.*;
import java.text.*;
import java.lang.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner A=new Scanner(System.in);
        int num=A.nextInt();
        int sum=0,rem;
        int org=num;
        while(num>0){
            rem=num%10;
            sum=sum+rem;
            num=num/10;
        }
        if(org%sum==0){
            System.out.println("Harshad Number");
        }
        else{
            System.out.println("Not Harshad Number");
        }
        
    }
}
