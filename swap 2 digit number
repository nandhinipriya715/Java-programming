import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner A=new Scanner(System.in);
        String a=A.nextLine();
        
        if(a.length()>=2){
            char fr=a.charAt(0);
            char sec=a.charAt(1);
            System.out.println(""+sec+fr);
        }
        else{
            System.out.println("STDOUT");
        }
        }
    }
