import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String []args) {
        Scanner A=new Scanner(System.in);
        int a=A.nextInt();
        if(a!=0){
            if(a>0)
            {
                System.out.println("positive");
            }
            else{
                System.out.println("negative");
            }
        }
        else{
            System.out.println("zero");
        }
        }
        
        }
